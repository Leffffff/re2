{
  "name": "@dashevo/wasm-re2",
  "version": "1.0.2",
  "description": "RE2 for different platforms (Linux, Windows, macOS)",
  "main": "./lib/scripts/re2.js",
  "types": "./lib/scripts/re2.d.ts",
  "jest": {
    "testRegex": ".test\\.js",
    "moduleFileExtensions": [
      "js",
      "ts",
      "cjs",
      "mjs"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "bin"
    ],
    "transform": {},
    "testEnvironment": "jest-environment-node"
  },
  "scripts": {
    "prepublishOnly": "npm test",
    "test": "npm run build && jest",
    "compile": "./compile.sh",
    "foundation": "npm run build && node lib/scripts/updateBuild.js",
    "build:update": "npm run foundation && npm run compile && npm test",
    "build": "rm -rf ./lib ./dist && rollup --config",
    "patch": "node ./bundle_wasm_for_web.js",
    "lint": "eslint ./scripts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Leffffff/re2.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.28",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@typescript-eslint/typescript-estree": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "prettier": "^2.2.1",
    "rollup": "^2.39.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "4.1.5"
  }
}
