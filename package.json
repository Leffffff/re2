{
  "name": "@skyfence/re2-wasm",
  "version": "1.0.0",
  "description": "RE2 for different platforms (Linux, Windows, macOS)",
  "main": "./lib/scripts/re2.js",
  "types": "./lib/scripts/re2.d.ts",
  "scripts": {
    "test": "npx tsc && jest",
    "compile": "./compile.sh",
    "foundation": "npx tsc && node lib/scripts/updateBuild.js",
    "build:update": "npm run foundation && npm run compile && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://intersog.skyfence.com/vzhehlov/re2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^26.0.3",
    "chalk": "^4.1.0",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.1"
  },
  "devDependencies": {
    "@types/node": "^13.13.12",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "@typescript-eslint/typescript-estree": "^2.9.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "typescript": "3.8.3"
  },
  "jest": {
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)x?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "/lib/",
      "/node_modules/",
      "/bin/"
    ]
  }
}